# üåç Local Environment Variables for WEM Dashboard
# Copy this to .env in your project root

# Application Info
VITE_APP_NAME="WEM Energy Dashboard"
VITE_APP_VERSION="1.0.0"

# Environment
NODE_ENV="development"

# API Configuration - Backend runs on port 5000
VITE_API_BASE_URL="http://localhost:5000/api"
VITE_WS_URL="ws://localhost:5000/ws/energy-data"

# Features
VITE_ENABLE_REAL_TIME="true"
VITE_ENABLE_NOTIFICATIONS="true"

# Development
VITE_DEBUG="true"
VITE_MOCK_DATA="true"