    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:websocket": "jest --testNamePattern=websocket",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean:win": "if exist dist rmdir /s /q dist && if exist .vite rmdir /s /q .vite && if exist node_modules\\.vite rmdir /s /q node_modules\\.vite",
    "clean:unix": "rm -rf dist .vite node_modules/.vite",
    "clean": "npm run clean:win 2>nul || npm run clean:unix",
    "prepare": "echo Prepare step completed",
    "postinstall": "echo Postinstall completed - Enhanced WEM Dashboard v3.0.0",
    "deps:update": "npm update",
    "deps:check": "npm outdated",
    "deps:audit": "npm audit",
    "start:enhanced": "echo Starting Enhanced WEM Dashboard... && npm run dev",
    "build:enhanced": "echo Building Enhanced WEM Dashboard... && npm run build",
    "demo:grid": "echo Demo: Interactive Grid Management available at /site/demo/grid",
