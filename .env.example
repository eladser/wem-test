# üåç Environment Variables for WEM Dashboard

# Application Info
REACT_APP_NAME="WEM Energy Dashboard"
REACT_APP_VERSION="1.0.0"
REACT_APP_DESCRIPTION="Wise Energy Management Dashboard"

# Environment
REACT_APP_ENVIRONMENT="development"

# API Configuration
REACT_APP_API_BASE_URL="http://localhost:3001/api"
REACT_APP_WS_URL="ws://localhost:3001/ws"

# Authentication
REACT_APP_AUTH_PROVIDER="local"
REACT_APP_JWT_SECRET="your-jwt-secret-here"

# Features
REACT_APP_ENABLE_REAL_TIME="true"
REACT_APP_ENABLE_NOTIFICATIONS="true"
REACT_APP_ENABLE_EXPORT="true"
REACT_APP_ENABLE_PERFORMANCE_MONITORING="true"

# Theme
REACT_APP_DEFAULT_THEME="dark"
REACT_APP_ENABLE_THEME_SWITCHING="true"

# Monitoring & Analytics
REACT_APP_ENABLE_ANALYTICS="false"
REACT_APP_ANALYTICS_ID=""
REACT_APP_SENTRY_DSN=""

# Development
REACT_APP_DEBUG="true"
REACT_APP_MOCK_DATA="true"

# Build Settings
GENERATE_SOURCEMAP="true"
REACT_APP_BUILD_DATE=""