# üåç Environment Variables for WEM Dashboard

# Application Info
VITE_APP_NAME="WEM Energy Dashboard"
VITE_APP_VERSION="1.0.0"
VITE_APP_DESCRIPTION="Wise Energy Management Dashboard"

# Environment
NODE_ENV="development"

# API Configuration
VITE_API_BASE_URL="http://localhost:5000/api"
VITE_WS_URL="ws://localhost:5000/ws/energy-data"

# Authentication
VITE_AUTH_PROVIDER="local"
VITE_JWT_SECRET="your-jwt-secret-here"

# Features
VITE_ENABLE_REAL_TIME="true"
VITE_ENABLE_NOTIFICATIONS="true"
VITE_ENABLE_EXPORT="true"
VITE_ENABLE_PERFORMANCE_MONITORING="true"

# Theme
VITE_DEFAULT_THEME="dark"
VITE_ENABLE_THEME_SWITCHING="true"

# Monitoring & Analytics
VITE_ENABLE_ANALYTICS="false"
VITE_ANALYTICS_ID=""
VITE_SENTRY_DSN=""

# Development
VITE_DEBUG="true"
VITE_MOCK_DATA="true"

# API Configuration (Vite format)
VITE_API_TIMEOUT="10000"
VITE_API_RETRY_ATTEMPTS="3"

# Build Settings
GENERATE_SOURCEMAP="true"