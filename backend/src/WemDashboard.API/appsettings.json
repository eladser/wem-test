{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=wemdashboard.db;Foreign Keys=true;",
    "SqlServer": "Server=localhost;Database=WemDashboard;Trusted_Connection=true;TrustServerCertificate=true;",
    "PostgreSQL": "Host=localhost;Database=WemDashboard;Username=postgres;Password=yourpassword",
    "MySQL": "Server=localhost;Database=WemDashboard;Uid=root;Pwd=yourpassword;",
    "Redis": "localhost:6379"
  },
  "DatabaseProvider": "SQLite",
  "Jwt": {
    "Key": "wem-dashboard-super-secret-jwt-key-for-development-minimum-32-characters-long",
    "Issuer": "WemDashboard",
    "Audience": "WemDashboard",
    "ExpirationInMinutes": 60
  },
  "AllowedHosts": "*"
}
